
{
  "manifest_version": 2,
  "name": "Cache-22",
  "version": "1.0.3",
  "description": "Scrapes web sites and uploads the contents to the Cache-22 service.",
  "browser_action": {
    "default_title": "Cache-22",
    "default_popup": "popup.html",
    "default_icon": "cache-22-180.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs"
  ]
}